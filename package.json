{
  "name": "typescript-server-officex",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "newman": "newman run postman/postman-rest-api.json",
    "test": "npm run build:ts && tsc -p test/tsconfig.json && c8 node --test -r ts-node/register \"test/**/*.ts\"",
    "start": "npm run build:ts && node dist/server.js",
    "build": "tsc && npm run copy-assets",
    "build:ts": "tsc && npm run copy-assets",
    "watch:ts": "tsc -w && npm run copy-assets",
    "copy-assets": "cpx \"src/schema/**/*.sql\" dist/schema",
    "dev": "npm run build:ts && nodemon -L --watch src --ext ts,js,sql --exec \"npm run build:ts && node dist/server.js\"",
    "dev:start": "nodemon -L --watch dist --exec \"node dist/server.js\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.846.0",
    "@aws-sdk/s3-presigned-post": "^3.846.0",
    "@aws-sdk/s3-request-presigner": "^3.846.0",
    "@dfinity/principal": "^2.4.1",
    "@fastify/autoload": "^6.0.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/sensible": "^6.0.0",
    "@fastify/websocket": "^11.2.0",
    "@noble/ed25519": "^2.3.0",
    "@noble/hashes": "^1.8.0",
    "@officexapp/types": "^0.0.225",
    "@sentry/cli": "^2.51.1",
    "@sentry/node": "^10.3.0",
    "@sentry/profiling-node": "^10.3.0",
    "base64-js": "^1.5.1",
    "better-sqlite3": "^12.2.0",
    "bip39": "^3.1.0",
    "dotenv": "^17.2.0",
    "ethers": "^6.15.0",
    "fastify": "^5.0.0",
    "fastify-cli": "^7.4.0",
    "fastify-plugin": "^5.0.0",
    "lib0": "^0.2.114",
    "mixpanel": "^0.18.1",
    "node-cache": "^5.1.2",
    "node-fetch": "^3.3.2",
    "random-words": "^2.0.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.1.0",
    "@types/node-cache": "^4.1.3",
    "c8": "^10.1.2",
    "concurrently": "^9.0.0",
    "cpx": "^1.5.0",
    "fastify-tsconfig": "^3.0.0",
    "nodemon": "^3.1.10",
    "ts-node": "^10.4.0",
    "typescript": "~5.8.2"
  }
}
